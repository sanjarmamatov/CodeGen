{
  "name": "CodeGeneration",
  "version": "1.0.0",
  "license": "MIT",
  "browserslist": [
    "> 1%"
  ],
  "scripts": {
    "serve": "concurrently \"yarn dev:start\" \"yarn dev:start\" \"yarn dev:scss\" ",
    "dev:scss": "scss --watch ./src/scss/creative.scss:public/css/creative.css",
    "dev:node": "nodemon -e js,jsx,scss --ignore lib --ignore dist --exec babel-node src/server",
    "dev:wds": "webpack-dev-server --progress",
    "prod:build": "rimraf lib dist && babel src -d lib --ignore .test.js && NODE_ENV=production webpack -p --progress && scss src/scss/creative.scss public/css/creative.css && yarn pkg",
    "prod:start": "NODE_ENV=production pm2 start lib/server && pm2 logs",
    "prod:stop": "pm2 delete server",
    "lint": "eslint src webpack.config.babel.js --ext .js,.jsx",
    "test": "yarn lint && flow && jest --coverage",
    "prepack": "prepack lib/client/index.js",
    "pkg": "pkg ./lib/server/index.js --out-dir exe/"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.1",
    "compression": "^1.6.2",
    "express": "^4.15.2",
    "handlebars": "^4.0.6",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^3.2.1",
    "mongoose": "^4.9.3",
    "nodemailer": "^4.0.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-helmet": "^5.0.0-beta",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-motion": "^0.5.0",
    "react-router": "^4.0.0",
    "react-router-dom": "^4.0.0",
    "react-router-transition": "^0.0.6",
    "typed.js": "^1.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.4.0",
    "babel-plugin-flow-react-proptypes": "^0.21.0",
    "babel-preset-env": "^1.2.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.23.0",
    "cross-env": "^3.2.3",
    "eslint": "^3.17.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-compat": "^1.0.2",
    "eslint-plugin-flowtype": "^2.30.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2 || ^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "flow-bin": "^0.41.0",
    "husky": "^0.13.2",
    "jest": "^19.0.2",
    "nodemon": "^1.11.0",
    "pm2": "^2.4.2",
    "rimraf": "^2.6.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
  }
}
